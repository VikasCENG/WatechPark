<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#424242"
    tools:context=".ui.Help.HelpFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/editText15"
            android:layout_width="200dp"
            android:layout_height="41dp"
            android:layout_marginStart="48dp"
            android:ems="10"
            android:enabled="false"
            android:fontFamily="@font/quicksand"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:text="Register a Car to the system"
            android:textColor="#aed581"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editText14"
            android:layout_width="154dp"
            android:layout_height="37dp"
            android:layout_marginStart="52dp"
            android:ems="10"
            android:enabled="false"
            android:fontFamily="@font/quicksand"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:text="View account details"
            android:textColor="#aed581"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/textView16"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editText13"
            android:layout_width="306dp"
            android:layout_height="61dp"
            android:layout_marginStart="56dp"
            android:ems="10"
            android:enabled="false"
            android:fontFamily="@font/quicksand"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:text="View Lot Name, Cost, Proximity Data/Status of the the parking lot"
            android:textColor="#f4f4f4"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/textView14"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editText12"
            android:layout_width="288dp"
            android:layout_height="58dp"
            android:layout_marginStart="56dp"
            android:ems="10"
            android:enabled="false"
            android:fontFamily="@font/quicksand"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:text="View your reserved locations,select a lot and proceed to Payment"
            android:textColor="#f4f4f4"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:fontFamily="sans-serif"
            android:text="@string/view"
            android:textColor="#aed581"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/editText13"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:text="@string/add4"
            android:textColor="#aed581"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/editText15"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:fontFamily="sans-serif"
            android:text="@string/parking"
            android:textColor="#aed581"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/editText12"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:text="@string/reservespot"
            android:textColor="#aed581"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/editText11"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="136dp"
            android:layout_height="45dp"
            android:layout_marginStart="52dp"
            android:layout_marginBottom="12dp"
            android:fontFamily="@font/quicksand"
            android:text="@string/help"
            android:textColor="#90caf9"
            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/textView17"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:text="@string/acct_manage"
            android:textColor="#aed581"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/editText14"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/editText11"
            android:layout_width="312dp"
            android:layout_height="59dp"
            android:layout_marginStart="48dp"
            android:ems="10"
            android:enabled="false"
            android:fontFamily="@font/quicksand"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:text="@string/make_res"
            android:textColor="#f4f4f4"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>